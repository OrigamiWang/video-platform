<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="szu.se.mapper.SimpleUserMapper">


    <resultMap id="simpleUserMap" type="szu.se.dao.SimpleUser">
        <id column="id" property="id"/>
        <result column="username" property="username"/>
        <result column="password" property="password"/>
    </resultMap>

<!--    List<SimpleUser> getSimpleUserList();-->
    <select id="getSimpleUserList" resultMap="simpleUserMap">
        SELECT * FROM simple_user
    </select>


</mapper>